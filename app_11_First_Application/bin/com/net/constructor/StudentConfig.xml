<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="student" class="com.net.constructor.Student">
    <constructor-arg  value="333"></constructor-arg>
    <constructor-arg  value="venkat"></constructor-arg>
    <constructor-arg>
     <list>
      <value>leela</value>
      <value>jagadeesh</value>
     </list>
    </constructor-arg>
    
    <constructor-arg>
     <set>
     <value>jspider</value>
     <value>vidyanikethan</value>
     </set>
    </constructor-arg>
    
    <constructor-arg>
     <map>
     <entry key="3" value="leela"></entry>
     </map>
    </constructor-arg>
    
    <constructor-arg ref="address"></constructor-arg>
    </bean>
    
    
    <bean id="address" class="com.net.constructor.Address">
    <constructor-arg value="3"></constructor-arg>
    <constructor-arg value="tpt"></constructor-arg>
    </bean>
</beans>
